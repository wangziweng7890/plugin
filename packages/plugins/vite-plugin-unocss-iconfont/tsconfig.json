{
  "compilerOptions": {
    "target": "ES2022",
    "module": "esnext",
    "lib": ["es2022"],
    "strict": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "types": ["node"]
  },
  "include": ["src"]
}
