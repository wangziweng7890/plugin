{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": [
      "ES2020"
    ],
    "module": "ES2020",
    "moduleResolution": "node",
    "noEmit": true,
    "noEmitOnError": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "sourceMap": true,
    "strict": false,
    "target": "ES2020"
  },
  "ts-node": {
    "esm": true
  },
  "include": [
    "./"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "test/types"
  ]
}
